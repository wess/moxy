enum Color {
  Red,
  Green,
  Blue
}

enum Shape {
  Circle(float radius),
  Rect(int w, int h),
  None
}

void main() {
  Color c = Color::Red;
  int found = 0;
  match c {
    Color::Red => { found = 1; },
    Color::Green => { found = 2; },
    Color::Blue => { found = 3; },
  }
  assert(found == 1);

  Shape s = Shape::Circle(3.14);
  int is_circle = 0;
  match s {
    Shape::Circle(r) => { is_circle = 1; },
    Shape::Rect(w) => { is_circle = 0; },
    Shape::None => { is_circle = 0; },
  }
  assert(is_circle == 1);
}
