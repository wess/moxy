#include <string.h>

typedef struct {
  int x;
  int y;
} Point;

int point_sum(Point p) {
  return p.x + p.y;
}

void main() {
  // struct usage
  Point p;
  p.x = 10;
  p.y = 20;
  assert(point_sum(p) == 30);

  // switch/case passthrough
  int val = 2;
  int result = 0;
  switch (val) {
    case 1: result = 10; break;
    case 2: result = 20; break;
    default: result = 0; break;
  }
  assert(result == 20);

  // strlen from included header
  int len = strlen("hello");
  assert(len == 5);
}
