void main() {
    // range iteration
    int sum = 0;
    for i in 0..5 {
        sum += i;
    }
    assert(sum == 10)

    // list iteration
    int[] nums = [10, 20, 30];
    int total = 0;
    for n in nums {
        total += n;
    }
    assert(total == 60)

    // map iteration with key and value
    map[string,int] ages = {};
    ages.set("alice", 30);
    ages.set("bob", 25);

    int age_sum = 0;
    for k, v in ages {
        age_sum += v;
    }
    assert(age_sum == 55)

    // nested for..in (validates unique counters)
    int nested = 0;
    for i in 0..3 {
        for j in 0..4 {
            nested += 1;
        }
    }
    assert(nested == 12)

    print("all for..in tests passed")
}
