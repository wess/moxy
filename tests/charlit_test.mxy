#include <string.h>

void main() {
  char a = 'A';
  assert(a == 'A')

  char nl = '\n';
  assert(nl == '\n')

  char tab = '\t';
  assert(tab == '\t')

  char nul = '\0';
  assert(nul == '\0')

  char bs = '\\';
  assert(bs == '\\')

  char sq = '\'';
  assert(sq == '\'')

  // test null terminator usage
  char buf[8];
  memset(buf, 'x', 8);
  buf[3] = '\0';
  assert(strlen(buf) == 3)
}
