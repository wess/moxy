#include <stdlib.h>
#include <stdio.h>
#include <stdbool.h>



int main(void) {
    if (true) {
        printf("%s\n", "yes");
    } else {
        printf("%s\n", "no");
    }
    for (int i = 0; i < 3; i++) {
        printf("%d\n", i);
    }
    int x = 0;
    while (x < 2) {
        printf("%d\n", x);
        x++;
    }
    return 0;
}

