#include <stdlib.h>
#include <stdio.h>
#include <stdbool.h>

int double_it(int x);
int add(int a, int b);


int double_it(int x) {
    return x * 2;
}

int add(int a, int b) {
    return a + b;
}

int main(void) {
    int result = double_it(5);
    printf("%d\n", result);
    printf("%d\n", add(double_it(5), 3));
    return 0;
}

