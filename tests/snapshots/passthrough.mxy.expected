#include <string.h>
#include <stdlib.h>
#include <stdio.h>
#include <stdbool.h>

typedef struct {int x; int y;} Point;


int main(void) {
    Point p;
    p.x = 10;
    p.y = 20;
    int sum = p.x + p.y;
    printf("%d\n", sum);
    switch (sum) {case 30: print ("thirty"); break; default: print ("other"); break;}
    return 0;
}

