#include <stdio.h>
#include <stdlib.h>

void eprintln(string msg) {
  fprintf(stderr, "%s\n", msg);
}

string readln() {
  char buf[1024];
  if (fgets(buf, sizeof(buf), stdin) == NULL) {
    return "";
  }
  int len = strlen(buf);
  if (len > 0 && buf[len - 1] == '\n') {
    buf[len - 1] = '\0';
  }
  return strdup(buf);
}
