#!/usr/bin/env bash
set -euo pipefail

REPO="wess/moxy"

versions=$(
  gh api "repos/${REPO}/releases" --paginate --jq '.[].tag_name' 2>/dev/null \
  || curl -fsSL "https://api.github.com/repos/${REPO}/releases" \
       | grep '"tag_name"' | sed 's/.*"v\(.*\)".*/\1/' | sort -V
)

echo "${versions}" | sed 's/^v//' | tr '\n' ' '
